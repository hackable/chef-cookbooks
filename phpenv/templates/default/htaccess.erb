<IfModule mod_rewrite.c>
    RewriteEngine on
    
    RewriteCond $1 !^(index\.php|assets|robots\.txt|favicon\.ico)

    RewriteRule ^(.*)$ index.php/$1 [L]
    
    <IfModule mod_env.c>
        # Environment Variables for application <%= @application %>

      <% @env[@application.to_s][:values].sort.each do | env_value | %> 
        SetEnv <%= env_value %> 
      <% end %>
    </IfModule>
</IfModule>
